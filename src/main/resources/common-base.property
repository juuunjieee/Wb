#定义common-base用到的表存在的数据库对应的连接池名称，对应dbpool里的连接池名称
sys.dbpoolname=yscf_mdb

#登录短信验证规则，如果login.sms.test.to指定了手机号码，则发送给指定的手机号，可以为"no"，不需要手机验证码验证
login.sms.test.to=no
#在login.sms.test.to=no有效，指定一个固定的手机验证码，登录时输入此验证码，测通过验证
login.sms.no.smscode=123456
#短信验证码发送插件类，通过此类发送短信
login.sms.class=com.yscf.admin.utils.LogSmsImpl
#登录时构造UserInfo里的extInfo字段，扩展com.ulwxbase.web.action.utils.LoginUserInfPlugin的实现
login.userInf.plugin=com.yscf.admin.services.utils.LoginUserInfPluginImpl
#前端登录密码rsa加密私钥
private.
